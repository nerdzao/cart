{
  "env": {
    "es6": true,
    "jest": true,
    "node": true
  },
  "extends": ["airbnb-base"],
  "globals": {
    "Zone": true
  },
  "parserOptions": {
    "ecmaVersion": 2017
  },
  "settings": {
    "import/resolver": {
      "node": {
        "paths": ["src", "test"]
      }
    }
  },
  "rules": {
    "class-methods-use-this": "off",
    "import/prefer-default-export": "off"
  }
}
